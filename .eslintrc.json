{
    "extends":["google", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": [
            "error", {
                "singleQuote": true,
                "semi": false
            }
        ]
    }
}
